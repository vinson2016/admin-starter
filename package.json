{
    "name": "admin-starter",
    "version": "1.0.0",
    "description": "自用中台项目脚手架",
    "main": "./public/electron.js",
    "scripts": {
        "clean": "rimraf build",
        "lint": "eslint .",
        "lint:fix": "npm run lint -- --fix",
        "dll": "webpack --config ./scripts/pack.dll.js",
        "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./scripts/pack.dev.js",
        "build": "npm run clean && cross-env NODE_ENV=production webpack --config ./scripts/pack.build.js",
        "build:electron": "npm run clean && cross-env NODE_ENV=production webpack --config ./scripts/pack.electron.js",
        "electron-dev": "concurrently \"BROWSER=none npm run dev\" \"wait-on http://localhost:8088 && electron .\"",
        "pack:mac": "electron-builder --mac",
        "pack:win": "electron-builder --win",
        "pack:linux": "electron-builder --linux",
        "prepack:all": "npm run build:electron",
        "pack:all": "electron-builder -mwl",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
        "g": "npm run generate",
        "generate": "plop --plopfile ./generators/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rmlzy/admin-starter.git"
    },
    "pre-commit": [
        "lint"
    ],
    "author": "rmlzy",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/rmlzy/admin-starter/issues"
    },
    "homepage": "https://rmlzy.github.io/admin-starter",
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.3.0",
        "@babel/plugin-proposal-decorators": "^7.3.0",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/plugin-transform-runtime": "^7.2.0",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-react": "^7.0.0",
        "babel-loader": "^8.0.5",
        "babel-plugin-import": "^1.11.0",
        "concurrently": "^4.1.0",
        "copy-webpack-plugin": "^4.6.0",
        "cross-env": "^5.2.0",
        "css-loader": "^2.1.0",
        "electron": "^4.0.2",
        "electron-builder": "^20.38.5",
        "eslint": "^5.12.1",
        "eslint-config-standard": "^12.0.0",
        "eslint-config-standard-react": "^7.0.2",
        "eslint-plugin-babel": "^5.3.0",
        "eslint-plugin-import": "^2.15.0",
        "eslint-plugin-node": "^8.0.1",
        "eslint-plugin-promise": "^4.0.1",
        "eslint-plugin-react": "^7.12.4",
        "eslint-plugin-standard": "^4.0.0",
        "file-loader": "^3.0.1",
        "gh-pages": "^2.0.1",
        "html-webpack-plugin": "^3.2.0",
        "mini-css-extract-plugin": "^0.5.0",
        "node-sass": "^4.11.0",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "plop": "^2.2.0",
        "pre-commit": "^1.2.2",
        "rimraf": "^2.6.3",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "terser-webpack-plugin": "^1.2.1",
        "typescript": "^3.2.4",
        "wait-on": "^3.2.0",
        "webpack": "^4.29.0",
        "webpack-cli": "^3.2.1",
        "webpack-dev-server": "^3.1.14",
        "webpack-merge": "^4.2.1"
    },
    "dependencies": {
        "@alifd/next": "^1.11.11",
        "@babel/polyfill": "^7.2.5",
        "@babel/runtime": "^7.3.1",
        "@rematch/core": "^1.0.6",
        "@rematch/loading": "^1.1.2",
        "add-asset-html-webpack-plugin": "^3.1.2",
        "babel-eslint": "^10.0.1",
        "connected-react-router": "^6.2.2",
        "electron-is-dev": "^1.0.1",
        "history": "^4.7.2",
        "moment": "^2.24.0",
        "prop-types": "^15.6.2",
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "react-loadable": "^5.5.0",
        "react-redux": "^6.0.0",
        "react-router-dom": "^4.3.1",
        "redux": "^4.0.1"
    },
    "build": {
        "appId": "com.example.electron-cra",
        "productName": "AdminStarter-桌面版",
        "asar": false,
        "extends": null,
        "directories": {
            "buildResources": "dist"
        },
        "mac": {
            "icon": "build/images/logo.png"
        },
        "win": {
            "icon": "build/images/logo.ico"
        }
    }
}
